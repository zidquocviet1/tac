<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/color"
        android:bottom="6dp"
        android:left="6dp"
        android:right="6dp"
        android:top="6dp">
        <shape android:shape="oval">
            <solid android:color="@color/purple_500" />
        </shape>
    </item>
    <item android:id="@+id/outer">
        <shape android:shape="oval">
            <stroke
                android:width="3dp"
                android:color="#4d4d4d" />
        </shape>
    </item>
</layer-list>